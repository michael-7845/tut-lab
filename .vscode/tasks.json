{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ng-build",
      "type": "shell",
      "command": "ng build",
      "problemMatcher": [
        "$webpack-builder"
      ]
    },
    {
      "label": "ng-watch",
      "type": "shell",
      "command": "ng build --watch"
    },
    {
      "label": "spring-boot:run",
      "type": "shell",
      "command": "mvn spring-boot:run"
    },
  ]
}
